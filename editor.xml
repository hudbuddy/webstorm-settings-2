<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.05" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="ECMAScript 6" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="microservices.url.path.inlay.hints" />
    <settingsMapElement>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForShorthandFunctions" value="true" />
          <option name="showForVariablesAndFields" value="true" />
        </Settings>
      </TypeScript.ts.type.hints>
    </settingsMapElement>
  </component>
  <component name="LiveEditOptions" nodeUpdateOnChanges="true" />
  <component name="TodoConfiguration">
    <multiLine>false</multiLine>
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\:\b.*">
      <option name="FOREGROUND" value="45889a" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(\n\t\s)*\&quot;(.|\?|\n\t\s)*\&quot;">
      <option name="FOREGROUND" value="b3b1af" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="(\n\t|\s)*\&gt; \w*.*\n">
      <option name="FOREGROUND" value="9ec899" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="(\n\t\s)*- \w*.*\n">
      <option name="FOREGROUND" value="e3e3e3" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>