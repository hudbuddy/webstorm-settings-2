<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="LINE_SPACING" value="1.05" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="ECMAScript 6" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="LiveEditOptions" nodeUpdateOnChanges="true" useJBChromeExtension="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="45889a" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
</application>